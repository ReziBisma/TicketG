@model TicketGo.Models.Ticket
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Create new Ticket</h1>
</div>
<div>
    <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="tempatwisata" class="control-label"></label>
                <input asp-for="tempatwisata" class="form-control" />
                <span asp-validation-for="tempatwisata" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Waktu" class="control-label"></label>
                <input asp-for="Waktu" class="form-control" />
                <span asp-validation-for="Waktu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tranportasi" class="control-label"></label>
                <input asp-for="Tranportasi" class="form-control" />
                <span asp-validation-for="Tranportasi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Harga" class="control-label"></label>
                <input asp-for="Harga" class="form-control" />
                <span asp-validation-for="Harga" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Stock" class="control-label"></label>
                <input asp-for="Stock" class="form-control" />
                <span asp-validation-for="Stock" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
</div>
@section Scripts{}
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
